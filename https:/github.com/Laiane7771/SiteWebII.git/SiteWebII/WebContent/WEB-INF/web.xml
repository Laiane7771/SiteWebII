<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Ecommerce_Laiane</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Servlet Gerenciador Conexao</servlet-name>
    <servlet-class>edu.ifg.formosa.controll.GerenciadorConexaoServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <context-param>
    <param-name>usuario</param-name>
    <param-value>postgres</param-value>
  </context-param>
  <context-param>
    <param-name>senha</param-name>
    <param-value>12345</param-value>
  </context-param>
  <context-param>
    <param-name>host</param-name>
    <param-value>localhost</param-value>
  </context-param>
  <context-param>
    <param-name>db</param-name>
    <param-value>postgresql</param-value>
  </context-param>
  <context-param>
    <param-name>porta</param-name>
    <param-value>5432</param-value>
  </context-param>
  <context-param>
    <param-name>nomeDB</param-name>
    <param-value>BDSite</param-value>
  </context-param>
</web-app>